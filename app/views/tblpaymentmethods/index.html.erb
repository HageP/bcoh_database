<div class="page-header">
  <h1>
    Tblpaymentmethods
    <a href="/tblpaymentmethods/new" class="btn btn-lg btn-success">New Tblpaymentmethod</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Method</th>
        <th>Actions</th>
      </tr>

      <% @tblpaymentmethods.each do |tblpaymentmethod| %>
      <tr>
        <td><%= tblpaymentmethod.method %></td>
        <td>
          <a href="/tblpaymentmethods/<%= tblpaymentmethod.id %>" class="btn btn-primary">Show</a>
          <a href="/tblpaymentmethods/<%= tblpaymentmethod.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_tblpaymentmethod/<%= tblpaymentmethod.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
