<div class="page-header">
  <h1>
    Tbltenants
    <a href="/tbltenants/new" class="btn btn-lg btn-success">New Tbltenant</a>
  </h1>
</div><div class="row mb-1">
  <div class="col-md-12">
    <%= search_form_for @q, :class => "collapse", :id => "tbltenants_filters" do |f| %>
      <p class="lead">Narrow results:</p>

        <div class="form-group">
    <%= f.label :tenant_first_cont, "Tenant first contains" %>
    <%= f.text_field :tenant_first_cont, :class => "form-control", :placeholder => "Tenant first contains" %>
  </div>

<div class="form-group">
    <%= f.label :tenant_phone_cont, "Tenant phone contains" %>
    <%= f.text_field :tenant_phone_cont, :class => "form-control", :placeholder => "Tenant phone contains" %>
  </div>

<div class="form-group">
    <%= f.label :tenant_email_cont, "Tenant email contains" %>
    <%= f.text_field :tenant_email_cont, :class => "form-control", :placeholder => "Tenant email contains" %>
  </div>

<div class="form-group">
          <%= f.label :created_at, "Created at" %>
          <div class="row">
            <div class='col-md-6 mb-1'>
              <div class='input-group date' id='created_atgteqPicker'>
                  <%= f.text_field :created_at_gteq, :class => "form-control", :placeholder => "Created at greater than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>

            <div class='col-md-6'>
              <div class='input-group date' id='created_atlteqPicker'>
                  <%= f.text_field :created_at_lteq, :class => "form-control", :placeholder => "Created at less than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>
          </div>
      </div>
      <script type="text/javascript">
          $(function () {
              $('#created_atgteqPicker').datetimepicker({
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#created_atlteqPicker').datetimepicker({
                  useCurrent: false,
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#created_atgteqPicker').on("dp.change", function (e) {
                  $('#created_atlteqPicker').data("DateTimePicker").minDate(e.date);
              });
              $('#created_atlteqPicker').on("dp.change", function (e) {
                  $('#created_atgteqPicker').data("DateTimePicker").maxDate(e.date);
              });
          });
      </script>

        <div class="form-group">
          <%= f.label :invoice_prepared, "Tblinvoice Invoice prepared" %>
          <div class="row">
            <div class='col-md-6 mb-1'>
              <div class='input-group date' id='tblinvoicesgteqinvoice_preparedPicker'>
                  <%= f.text_field :tblinvoices_invoice_prepared_gteq, :class => "form-control", :placeholder => "Tblinvoice Invoice prepared greater than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>

            <div class='col-md-6'>
              <div class='input-group date' id='tblinvoiceslteqinvoice_preparedPicker'>
                  <%= f.text_field :tblinvoices_invoice_prepared_lteq, :class => "form-control", :placeholder => "Tblinvoice Invoice prepared less than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>
          </div>
      </div>
      <script type="text/javascript">
          $(function () {
              $('#tblinvoicesgteqinvoice_preparedPicker').datetimepicker({
                  format: 'MM/DD/YYYY'
              });
              $('#tblinvoiceslteqinvoice_preparedPicker').datetimepicker({
                  useCurrent: false,
                  format: 'MM/DD/YYYY'
              });
              $('#tblinvoicesgteqinvoice_preparedPicker').on("dp.change", function (e) {
                  $('#tblinvoiceslteqinvoice_preparedPicker').data("DateTimePicker").minDate(e.date);
              });
              $('#tblinvoiceslteqinvoice_preparedPicker').on("dp.change", function (e) {
                  $('#tblinvoicesgteqinvoice_preparedPicker').data("DateTimePicker").maxDate(e.date);
              });
          });
      </script>

      <%= f.submit :class => "btn btn-primary btn-block mb-1" %>

      <a href="/tbltenants" class="btn btn-default btn-block mb-1">Clear filters</a>
    <% end %>
    <a class="btn btn-default btn-block mb-1" data-toggle="collapse" href="#tbltenants_filters">
      Show Filters
    </a>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Tenant first</th>
        <th>Tenant email</th>
        <th>Tenant phone</th>
        <th>Actions</th>
      </tr>

      <% @tbltenants.each do |tbltenant| %>
      <tr>
        <td><%= tbltenant.tenant_first %></td>
        <td><%= tbltenant.tenant_email %></td>
        <td><%= tbltenant.tenant_phone %></td>
        <td>
          <a href="/tbltenants/<%= tbltenant.id %>" class="btn btn-primary">Show</a>
          <a href="/tbltenants/<%= tbltenant.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_tbltenant/<%= tbltenant.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @tbltenants, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
