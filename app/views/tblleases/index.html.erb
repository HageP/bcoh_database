<div class="page-header">
  <h1>
    Tblleases
    <a href="/tblleases/new" class="btn btn-lg btn-success">New Tbllease</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Apartment</th>
        <th>Lease start</th>
        <th>Lease end</th>
        <th>Daily rate</th>
        <th>Tenant</th>
        <th>Tenant last</th>
        <th>Deposit</th>
        <th>Actions</th>
      </tr>

      <% @tblleases.each do |tbllease| %>
      <tr>
        <td><%= tbllease.apartment_id %></td>
        <td><%= tbllease.lease_start %></td>
        <td><%= tbllease.lease_end %></td>
        <td><%= tbllease.daily_rate %></td>
        <td><%= tbllease.tenant_id %></td>
        <td><%= tbllease.tenant_last %></td>
        <td><%= tbllease.deposit %></td>
        <td>
          <a href="/tblleases/<%= tbllease.id %>" class="btn btn-primary">Show</a>
          <a href="/tblleases/<%= tbllease.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_tbllease/<%= tbllease.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
